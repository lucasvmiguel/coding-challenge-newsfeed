{
  "name": "code-challenge-newsfeed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "graphql:schema:cleanup": "rm -rf graphql/client/__generated__",
    "graphql:schema:download": "npx apollo service:download --endpoint=http://localhost:3000/api/graphql graphql-schema.json",
    "graphql:schema:generate": "npm run graphql:schema:cleanup && npm run graphql:schema:download && npx apollo client:codegen --target typescript --localSchemaFile graphql-schema.json --includes graphql/client/**/*.ts --globalTypesFile graphql/client/__generated__/globalTypes.d.ts --tsFileExtension d.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@types/marked": "^1.2.2",
    "apollo-server-micro": "^2.21.0",
    "graphql": "^15.5.0",
    "marked": "^2.0.0",
    "next": "11.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-loading": "^2.0.3",
    "sqlite3": "^5.0.2",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/react": "^12.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/sqlite3": "^3.1.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.1.5"
  }
}
